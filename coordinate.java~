public class coordinate{
    private int x, y;
    
    public ViewX(){
	return x;
    }

    public ViewY(){
	return y;
    }

    public void SetX(int z){
	x = z;
    }

    public void Sety(int z){
	y = z;
    }

    public void SetXY(int nx, int ny){
	x = nx;
	y = ny;
    }

    public toString(){
	return "(" + x + "," + y + ")";
    }

}
